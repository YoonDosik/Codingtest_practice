   "source": [
    "-- 코드를 입력하세요\n",
    "SELECT A.PRODUCT_CODE, sum(B.SALES_AMOUNT) * A.PRICE AS SALES\n",
    "FROM PRODUCT AS A LEFT JOIN OFFLINE_SALE AS B\n",
    "ON A.PRODUCT_ID = B.PRODUCT_ID\n",
    "GROUP BY A.PRODUCT_CODE\n",
    "ORDER BY SALES DESC, A.PRODUCT_CODE ASC"
   ],
   "metadata": {
